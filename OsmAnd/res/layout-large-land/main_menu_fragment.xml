    <ImageButton android:text="@string/music_button" android:id="@+id/musicButton" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight="1.0" android:src="@drawable/music_icon" android:gravity="center_vertical" android:background="@drawable/vertical_tab_button_shape"></ImageButton>
